{
  "name": "article",
  "plural": "articles",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "abstract": {
      "type": "string",
      "required": true
    },
    "key_words": {
      "type": "string",
      "required": true
    },
    "authors": {
      "type": [
        "string"
      ]
    },
    "author_id": {
      "type": "string",
      "required": true
    },
    "publishing_id": {
      "type": "string",
      "required": true
    },
    "date": {
      "type": "date",
      "required": true
    },
    "state": {
      "type": "string"
    },
    "file": {
      "type": "string",
      "required": true
    },
    "assessors": {
      "type": [
        "string"
      ]
    }
  },
  "validations": [],
  "relations": {
    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": "article_id"
    },
    "publishing": {
      "type": "belongsTo",
      "model": "publishing",
      "foreignKey": "publishing_id"
    },
    "author": {
      "type": "belongsTo",
      "model": "author",
      "foreignKey": "author_id"
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
